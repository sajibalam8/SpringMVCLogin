<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="studentdb" match-name="admin"/>
  <table-filter match-catalog="studentdb" match-name="course"/>
  <table-filter match-catalog="studentdb" match-name="course_student"/>
  <table-filter match-catalog="studentdb" match-name="course_teacher"/>
  <table-filter match-catalog="studentdb" match-name="student"/>
  <table-filter match-catalog="studentdb" match-name="student_teacher"/>
  <table-filter match-catalog="studentdb" match-name="teacher"/>
</hibernate-reverse-engineering>